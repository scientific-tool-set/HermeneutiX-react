{
  "name": "hermeneutix-react",
  "version": "0.1.0",
  "description": "HermeneutiX web application (React + Redux)",
  "main": "index.js",
  "repository": "git@github.com:scitos/HermeneutiX-react.git",
  "author": "SciToS (Carsten Wickner)",
  "license": "MIT",
  "scripts": {
    "start": "better-npm-run start",
    "test": "better-npm-run test",
    "build": "better-npm-run build"
  },
  "betterScripts": {
    "test": {
      "command": "node node_modules/karma-cli/bin/karma start karma.conf.js --single-run",
      "env": {
        "BABEL_ENV": "test"
      }
    },
    "start": {
      "command": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.config.js",
      "env": {
        "BABEL_ENV": "development"
      }
    },
    "build": {
      "command": "node node_modules/webpack/bin/webpack.js --config webpack.config.js --bail",
      "env": {
        "NODE_ENV": "build",
        "BABEL_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.0",
    "babel-preset-react-app": "^2.1.0",
    "better-npm-run": "0.0.14",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "2.13.1",
    "eslint-loader": "1.6.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-react": "6.4.1",
    "html-webpack-plugin": "^2.28.0",
    "jasmine": "^2.5.3",
    "json-loader": "^0.5.4",
    "karma": "0.13.22",
    "karma-cli": "1.0.0",
    "karma-eslint": "^2.2.0",
    "karma-firefox-launcher": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "1.7.0",
    "source-map-loader": "^0.1.6",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.1",
    "redux": "^3.0.4"
  }
}
